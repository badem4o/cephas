{
  "name": "cephas",
  "version": "3.1.1",
  "description": "Validate simple objects against a schema",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/.bin/mocha -R spec --recursive",
    "test:dev": "./node_modules/.bin/mocha -w -R spec --recursive",
    "coveralls": "./node_modules/.bin/nyc npm test && nyc report --reporter=text-lcov | ./node_modules/.bin/coveralls && rm -rf ./.nyc_output"
  },
  "keywords": [
    "validate",
    "validation",
    "form",
    "object",
    "micro",
    "library"
  ],
  "author": "milyord",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/milyord/cephas.git"
  },
  "bugs": {
    "url": "https://github.com/milyord/cephas/issues"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "coveralls": "^3.0.9",
    "eslint": "^6.8.0",
    "mocha": "^7.1.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^15.0.0"
  }
}
